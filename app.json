{
  "expo": {
    "name": "microfinancebank",
    "slug": "microfinancebank",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#fff"
    },
    "newArchEnabled": true,
    "bridgeless": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "AUDIO_RECORDING",
        "CAMERA",
        "CAMERA_ROLL",
        "CONTACTS",
        "LOCATION",
        "MICROPHONE",
        "PHONE_STATE",
        "READ_EXTERNAL_STORAGE",
        "RECORD_AUDIO",
        "VIDEO_LIBRARY",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/icon.png"
    }
  }
}
